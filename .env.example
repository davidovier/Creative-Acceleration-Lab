# Supabase Database Configuration
# Copy this file to .env and fill in your credentials
# DO NOT commit .env to version control

# Supabase Project URL
SUPABASE_URL=https://idqhczkvuoxetllmooch.supabase.co

# Supabase API Keys
# Anon/Public Key - Safe to use in client-side code (browsers, mobile apps)
SUPABASE_ANON_KEY=your_anon_key_here

# Service Role Key - SECRET! Only use server-side, has admin privileges
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Database Password
DB_PASSWORD=your_password_here

# Session Pooler Connection String
# Use this for serverless/edge functions or high-concurrency applications
DATABASE_URL=postgresql://postgres.idqhczkvuoxetllmooch:v8.pG_jZD5wdK%&@aws-1-eu-central-1.pooler.supabase.com:5432/postgres

# Direct Connection String (for migrations, admin tasks)
# Note: Direct connections have a limit, use pooler for application code
DIRECT_DATABASE_URL=postgresql://postgres.idqhczkvuoxetllmooch:v8.pG_jZD5wdK%&@aws-1-eu-central-1.pooler.supabase.com:6543/postgres

# Connection Pool Settings
PGBOUNCER_POOL_MODE=transaction
MAX_CONNECTIONS=100

# ============================================================================
# AI API KEYS (for RAG system)
# ============================================================================

# OpenAI API Key - For embeddings (text-embedding-3-small)
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key - For Claude agents
# Get your key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Voyage AI (alternative embeddings provider)
# VOYAGE_API_KEY=your_voyage_api_key_here

# ============================================================================
# CONFIGURATION
# ============================================================================

# Logging level: debug | info | warn | error
LOG_LEVEL=info
